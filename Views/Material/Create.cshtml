@model ManitasWeb.Models.Material

<div class="area-titulo">
    <h5 class="titulo-prim">Contactanos</h5>
</div>
<form asp-controller="Categoria" asp-action="Create">
    <div class="form-group">
        <label asp-for="NomMaterial" class="control-label">Nombre :</label>
        <input asp-for="NomMaterial" class="form-control" placeholder="Escriba su nombre" type="text" required />
    </div>
    <div class="form-group">
        <button type="submit" class="btn-prim form-control btn-form" type="text" required>Enviar</button>
    </div>
</form>